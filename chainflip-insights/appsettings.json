{
  "Bot": {
    "GraphUrl": "https://explorer-service-processor.chainflip.io/graphql",
    "ExplorerUrl": "https://scan.chainflip.io/swaps/",
    "SwapInfoDelay": 1000,
    "LastSwapIdLocation": "/tmp/chainflip-insights-swap-id"
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "ChainflipInsights": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
        }
      }
    ],
    "Properties": {
      "Application": "chainflip-insights"
    }
  }
}